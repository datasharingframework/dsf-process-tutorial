<ValueSet xmlns="http://hl7.org/fhir">
    <meta>
        <profile value="http://dsf.dev/fhir/StructureDefinition/value-set" />
        <tag>
            <system value="http://dsf.dev/fhir/CodeSystem/read-access-tag" />
            <code value="ALL" />
        </tag>
    </meta>
    <url value="http://tutorial.org/fhir/ValueSet/voting-results"/>
    <!-- version managed by bpe -->
    <version value="#{version}" />
    <name value="Voting Result"/>
    <title value="Voting Result ValueSet"/>
    <!-- status managed by bpe -->
    <status value="unknown" />
    <experimental value="false"/>
    <!-- date managed by bpe -->
    <date value="#{date}"/>
    <publisher value="DSF"/>
    <description value="ValueSet with values for voting results."/>
    <immutable value="true"/>
    <compose>
        <include>
            <system value="http://tutorial.org/fhir/CodeSystem/voting-process"/>
            <version value="#{version}"/>
            <concept>
                <code value="yes"/>
            </concept>
            <concept>
                <code value="no"/>
            </concept>
            <concept>
                <code value="timeout"/>
            </concept>
            <concept>
                <code value="result-bundle"/>
            </concept>
        </include>
    </compose>
</ValueSet>