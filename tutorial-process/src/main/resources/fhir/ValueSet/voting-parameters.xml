<ValueSet xmlns="http://hl7.org/fhir">
    <meta>
        <profile value="http://dsf.dev/fhir/StructureDefinition/value-set" />
        <tag>
            <system value="http://dsf.dev/fhir/CodeSystem/read-access-tag" />
            <code value="ALL" />
        </tag>
    </meta>
    <url value="http://tutorial.org/fhir/ValueSet/voting-parameters"/>
    <!-- version managed by bpe -->
    <version value="#{version}" />
    <name value="Voting Parameters"/>
    <title value="Voting Parameters ValueSet"/>
    <!-- status managed by bpe -->
    <status value="unknown" />
    <experimental value="false"/>
    <!-- date managed by bpe -->
    <date value="#{date}"/>
    <publisher value="DSF"/>
    <description value="ValueSet with values for task input/output parameter names for tasks related to voting."/>
    <immutable value="true"/>
    <compose>
        <include>
            <system value="http://tutorial.org/fhir/CodeSystem/voting-process"/>
            <version value="#{version}"/>
            <concept>
                <code value="binary-question" />
            </concept>
            <concept>
                <code value="vote" />
            </concept>
            <concept>
                <code value="voting-result"/>
            </concept>
        </include>
    </compose>
</ValueSet>